<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="icapture.hibernate">
  <class name="GenotypingRun" table="tblgenotypingruns">
    <id column="genotypingRunID" name="id">
      <generator class="native"/>
    </id>
    <property column="genotypingRunID" insert="false" name="genotypingRunID" not-null="true" update="false"/>
    <property column="description" name="description" not-null="true"/>
    <property column="date" name="date" not-null="true"/>
  </class>
</hibernate-mapping>
