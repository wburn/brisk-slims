<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="icapture.hibernate">
  <class name="Snp" table="tblsnp">
    <id column="snpID" name="id">
      <generator class="native"/>
    </id>
    <property column="snpID" insert="false" name="snpID" not-null="true" update="false"/>
    <property column="Chromosome" name="Chromosome" not-null="false"/>
    <property column="pos" name="pos" not-null="false"/>
    <property column="geneID" name="geneID" not-null="false"/>
    <property column="fxn_Class" name="fxnClass" not-null="false"/>
    <property column="rsNumber" name="rsNumber" not-null="true"/>
    <property column="majorAllele" name="majorAllele" not-null="false"/>
    <property column="minorAllele" name="minorAllele" not-null="false"/>
    <property column="strand" name="strand" not-null="false"/>
  </class>
</hibernate-mapping>
