<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="icapture.hibernate">
  <class name="Cohort" table="tblcohortlookup">
    <id column="cohortID" name="id">
      <generator class="native"/>
    </id>
    <property column="cohortID" insert="false" name="cohortID" not-null="true" update="false"/>
    <property column="name" name="description" not-null="true"/>
    <property column="sortOrder" name="sortOrder" not-null="true"/>
  </class>
</hibernate-mapping>
