<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="icapture.hibernate">
  <class name="Control" table="tblcontrol">
    <id column="controlID" name="id">
      <generator class="native"/>
    </id>
    <property column="controlID" insert="false" name="controlID" not-null="true" update="false"/>
    <property column="description" name="description" not-null="true"/>
    <property column="type" name="controlType" not-null="true"/>
  </class>
</hibernate-mapping>
