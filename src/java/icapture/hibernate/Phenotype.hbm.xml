<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="icapture.hibernate">
  <class name="Phenotype" table="tblPhenotypeLookup">
    <id column="phenotypeID" name="id">
      <generator class="native"/>
    </id>
    <property column="phenotypeID" insert="false" name="phenotypeID" not-null="true" update="false"/>
    <property column="name" name="name" not-null="true"/>
    <property column="type" name="type" not-null="true"/>
    <property column="description" name="description" not-null="true"/>
    <property column="abreviation" name="abbreviation" not-null="false"/>
  </class>
</hibernate-mapping>
