<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="icapture.hibernate">
  <class name="ContainerType" table="tblcontainertypes">
    <id column="containerTypeID" name="id">
      <generator class="native"/>
    </id>
    <property column="containerTypeID" insert="false" name="containerTypeID" not-null="true" update="false"/>
    <property column="description" name="description" not-null="true"/>
    <property column="rows" name="rows" not-null="true"/>
    <property column="columns" name="columns" not-null="true"/>
    <property column="sortOrder" name="sortOrder" not-null="true"/>
  </class>
</hibernate-mapping>
