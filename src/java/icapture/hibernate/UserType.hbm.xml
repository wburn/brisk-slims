<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="icapture.hibernate">
  <class name="UserType" table="tblusertypes">
    <id column="userTypeID" name="id">
      <generator class="native"/>
    </id>
    <property column="userTypeID" insert="false" name="userTypeID" not-null="true" update="false"/>
    <property column="description" name="description" not-null="true"/>
  </class>
</hibernate-mapping>
