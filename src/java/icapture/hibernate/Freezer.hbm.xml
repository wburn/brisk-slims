<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="icapture.hibernate">
  <class name="Freezer" table="tblfreezers">
    <id column="freezerID" name="freezerID">
      <generator class="native"/>
    </id>
    <property column="freezerID" insert="false" name="id" not-null="true" update="false"/>
    <property column="description" name="description" not-null="true"/>
    <property column="location" name="location" not-null="true"/>
    <property column="temperature" name="temperature" not-null="true"/>
    <property column="sortOrder" name="sortOrder" not-null="true"/>
  </class>
</hibernate-mapping>
